{
  "name": "simple-todo",
  "version": "0.1.0",
  "description": " Express.js / React.js / TypeScript => Simple ToDo",
  "scripts": {
    "build": "run-p build:*",
    "build:tsc": "tsc -p src/server",
    "build:webpack": "webpack --config src/client/webpack.config.js",
    "build:sass": "node-sass src/styles -o app/styles",
    "build:views": "cpx \"src/views/**/*.pug\" app/views",
    "clean": "rimraf app/server app/styles app/views app/scripts",
    "start": "node app/server/index.js"
  },
  "author": "cwd-k2",
  "license": "MIT",
  "dependencies": {
    "cpx": "^1.5.0",
    "express": "^4.17.1",
    "npm-run-all": "^4.1.5",
    "pug": "^3.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.7",
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "node-sass": "^4.14.1",
    "source-map-loader": "^1.0.1",
    "ts-loader": "^8.0.1",
    "typescript": "^3.9.7",
    "webpack": "^4.44.0",
    "webpack-cli": "^3.3.12"
  }
}
